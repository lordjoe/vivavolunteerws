{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
 //   "allowJs": true,
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
 //   "module": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
   "rootDir": "./src",
   "outDir": "./outsrc",
    "sourceMap": true,
    "isolatedModules": true,
    "noEmit": true,
//    "jsx": "react-jsx"
    "jsx": "react"
  },
  "include": [
    "src"
  ]
}
